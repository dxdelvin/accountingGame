{
  "dialogues": [
    {"id": 1, "type": "Normal", "text": "Welcome to {BrandName} Store!", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 2},
    {"id": 2, "type": "Normal", "text": "You're starting a business!", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 3},
    {"id": 3, "type": "YesNo", "text": "You're going to earn billions in business!", "options": ["Yes", "No"], "characterImagePath": "/Images/Store/charHappy.png", "nextId": {"Yes": 4, "No": 5}},
    {"id": 4, "type": "Normal", "text": "Yeah, we need a scorecard to maintain our transactions!", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 7},
    {"id": 5, "type": "Normal", "text": "Oh, come on, be positive!", "nextId": 6},
    {"id": 6, "type": "Normal", "text": "We do need a scorecard to maintain our transactions though!", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 7},
    {"id": 7, "type": "YesNo", "text": "You should check your piggy bank.", "options": ["Fine", "Why?"], "characterImagePath": "/Images/Store/Char@2x.png", "nextId": {"Fine": 8, "Why?": 9}},
    {"id": 8, "type": "Normal", "text": "You found 5 Euros!", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 10},
    {"id": 9, "type": "Normal", "text": "You need to invest in your business, so you found 5 Euros in your piggy bank.", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 10},
    {"id": 10, "type": "Normal", "text": "We should name something instead of Piggy Bank.", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 11},
    {"id": 11, "type": "Normal", "text": "How about 'Original Investment'?", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 12},
    {"id": 12, "type": "YesNo", "text": "Who owns the Original Investment?", "options": ["You?", "Myself!"], "characterImagePath": "/Images/Store/Char@2x.png", "nextId": {"You?": 13, "Myself!": 14}},
    {"id": 13, "type": "Normal", "text": "Thanks! But it's your hard-earned money, so you own it, not me!", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 15},
    {"id": 14, "type": "Normal", "text": "Correct!", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 15},
    {"id": 15, "type": "Normal", "text": "We need to put this in the scoreboard.", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 16},
    {"id": 16, "type": "Normal", "text": "So this should be on the right side of your scoreboard.", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 17},
    {"id": 17, "type": "Normal", "text": "There's a rule in a scoreboard.", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 18},
    {"id": 18, "type": "Normal", "text": "ALWAYS REMEMBER IT!!", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 19},
    {"id": 19, "type": "Normal", "text": "The left side always equals the right side.", "characterImagePath": "/Images/Store/Char@2x.png", "nextId": 20}
  ]
